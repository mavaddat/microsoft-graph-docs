{
  "schemaConfig": {
    "defaultNamespace": "microsoft.graph",
    "baseUrls": [
      "https://graph.microsoft.com/v1.0",
      "https://graph.microsoft.com/beta",
      "/v1.0",
      "/beta"
    ],
    "notLowerCamel": [
      "Json"
    ],
    "requiredYamlHeaders": [
      "title",
      "description",
      "author",
      "doc_type",
      "ms.subservice"
    ],
    "treatErrorsAsWarningsWorkloads": [
      "intune"
    ],
    "skipPermissionTableUpdateForWorkloads": [
      "intune"
    ],
    "foldersToSkip": [
      "templates",
      "includes"
    ],
    "filesToSkip": [
      "changelog.md",
      "contributing.md",
      "issue_template.md",
      "readme.md",
      "license.md",
      "index.md"
    ],
    "textToRemoveFromPropertyNames": [
      "(deprecated)"
    ] 
  },
  "schemaDiffConfig": {
    "dropElementsContaining": [
      "microsoft.graph.workbookFunctions",
      "microsoft.graph.workbookFunctionResult",
      "microsoft.graph.workbookFilter"
    ],
    "keepElementsContaining": [
      "workbook"
    ]
  }
}
